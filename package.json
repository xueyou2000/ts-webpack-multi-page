        "html-webpack-plugin": "^3.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.8.0",
        "jest-svg-transformer": "^1.0.0",
        "mini-css-extract-plugin": "^0.7.0",
        "mocker-api": "^1.7.6",
        "node-sass": "^4.12.0",
        "object-assign": "^4.1.1",
        "optimize-css-assets-webpack-plugin": "^5.0.1",
