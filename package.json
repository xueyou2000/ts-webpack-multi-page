{
    "name": "ts-webpack-multi-page",
    "version": "0.0.4",
    "description": "Webpack多页项目模板, 基于typescript编写webpack配置",
    "main": "index.js",
    "author": "XueYou",
    "license": "MIT",
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "scripts": {
        "test": "jest",
        "coverage": "jest --coverage",
        "set-webpack-env": "cross-env TS_NODE_PROJECT=\"tsconfigwebpack.json\"",
        "dev": "yarn run set-webpack-env cross-env WEBPACK_ENV=dev cross-env webpack-dev-server",
        "build-pro": "yarn run set-webpack-env cross-env WEBPACK_ENV=prod webpack --hide-modules --display none",
        "build-uat": "yarn run set-webpack-env cross-env WEBPACK_ENV=uat webpack --hide-modules --display none",
        "build:polyfill": "yarn run set-webpack-env webpack --config webpack.polyfill.config.ts --progress"
    },
    "devDependencies": {
        "@types/case-sensitive-paths-webpack-plugin": "^2.1.2",
        "@types/classnames": "^2.2.7",
        "@types/clean-webpack-plugin": "^0.1.3",
        "@types/copy-webpack-plugin": "^5.0.0",
        "@types/friendly-errors-webpack-plugin": "^0.1.1",
        "@types/glob": "^7.1.1",
        "@types/hard-source-webpack-plugin": "^1.0.1",
        "@types/html-webpack-plugin": "^3.2.0",
        "@types/jest": "^24.0.13",
        "@types/mini-css-extract-plugin": "^0.2.0",
        "@types/node": "^12.0.3",
        "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
        "@types/react": "^16.8.19",
        "@types/react-dom": "^16.8.4",
        "@types/webpack": "^4.4.32",
        "@types/webpack-bundle-analyzer": "^2.13.1",
        "@types/webpack-dev-server": "^3.1.6",
        "@types/webpack-env": "^1.13.9",
        "@types/webpack-manifest-plugin": "^2.0.0",
        "awesome-typescript-loader": "^5.2.1",
        "case-sensitive-paths-webpack-plugin": "^2.2.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.3",
        "core-js": "^3.1.3",
        "cross-env": "^5.2.0",
        "css-loader": "^2.1.1",
        "eruda-webpack-plugin": "^1.2.0",
        "file-loader": "^3.0.1",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "glob": "^7.1.4",
